<Window x:Class="MagnetYoke_HMI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MagnetYoke_HMI"
        xmlns:FM_View ="clr-namespace:FM2002.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="13*"/>
        </Grid.ColumnDefinitions>
        
        <Button Grid.Column="0" x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="16,21,0,0" VerticalAlignment="Top" Height="39" Width="71" Click="button_Click"/>
        <Grid Grid.Column="1">
            <FM_View:FM2002_View/>
        </Grid>
    </Grid>
</Window>
